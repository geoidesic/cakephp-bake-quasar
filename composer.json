{
    "name": "geoidesic/cakphp-bake-quasar",
    "description": "A bake plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Noel da Costa",
            "homepage": "https://www.arcdigital.uk",
            "role": "Author"
        }
    ],
    "keywords": [
        "CakePHP",
        "Quasar",
        "Bake"
    ],
    "support": {
        "source": "https://github.com/geoidesic/cakphp-bake-quasar",
        "issues": "https://github.com/geoidesic/cakphp-bake-quasar/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "QuasarAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuasarAdmin\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
